{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/login",
            "destination": "/pages/public/login.html"
        }
    ]
}